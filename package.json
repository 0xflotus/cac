  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "operator-linebreak": [
        2,
        "before"
      ],
      "no-warning-comments": [
        2,
        {
          "terms": [
            "fixme"
          ],
          "location": "start"
        }
      ]
    }
  },
  "dependencies": {
    "camelcase-keys": "^2.1.0",
    "co": "^4.6.0",
    "deep-assign": "^2.0.0",
    "is-generator-function": "^1.0.3",
    "minimist": "^1.2.0",
    "read-pkg-up": "^1.0.1"
  }
